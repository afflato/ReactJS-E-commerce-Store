{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "headers":{
        "Access-Control-Allow-Origin": "https://react-js-e-commerce-store-client.vercel.app",
        "Access-Control-Allow-Credentials":true
      },
      "dest": "/auth.routes.js"
    },
    {
      "src": "/user/(.*)",
      "dest": "/user.routes.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/admin.routes.js"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ]
}
